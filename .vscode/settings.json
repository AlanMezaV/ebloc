{
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.format": "explicit"
  },
  "cSpell.words": [
    "Cloudinary",
    "clsx",
    "codegen",
    "ebloc",
    "fontsource",
    "hookform",
    "lucide",
    "pago",
    "sonner",
    "tailwindcss",
    "tanstack",
    "typeorm"
  ]
}
